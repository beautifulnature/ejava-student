<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
        version="2.0">
    
    <!-- This provides an example of using a primary key class which
        has fields that match the fields of this class.  
        -->
    <entity class="ejava.examples.orm.core.mapped.Mower" 
            access="FIELD"
            metadata-complete="true"
            name="MappedMower">
        <table name="ORMCORE_MOWER"/>
        <id-class class="ejava.examples.orm.core.MowerPK"/>
        <attributes>
            <id name="make"/>
            <id name="model"/>
        </attributes>
    </entity>    
</entity-mappings>
    
    
